
<%=render :partial => "boolean_response", :locals => {f:f }%>
<div class="row js-answer hidden" data-answer-type="date">
  <div class="js-date-responses">
    <% f.object.responses.each do |response| %>
      <%= f.fields_for :responses, response do |response_fields| %>
      <% end %>
    <% end %>
  <%= f.fields_for :responses, f.object.responses.build do |response_fields| %>
    <%=render :partial => "date_response", :locals => {f: response_fields}%>
  <% end %>
    <div class= "col-md-12">
      <a data-action="qle#addDateResponse">Add Responses to this question</a>
    </div>
  </div>
</div>
<div class="row js-answer hidden" data-answer-type="multiple_choice">
  <div class="js-multiple-choice-responses">
    <% f.object.responses.each do |response| %>
      <%= f.fields_for :responses, response do |response_fields| %>
        <%=render :partial => "multiple_choice_response", :locals => {response_fields: response_fields}%>
      <% end %>
    <% end %>
  </div>
  <%= f.fields_for :responses, f.object.responses.build do |response_fields| %>
    <%=render :partial => "multiple_choice_response", :locals => {response_fields: response_fields}%>
  <% end %>
    <div class="col-md-12">
      <a data-action="qle#addMultipleChoiceResponse">Add Responses to this question</a>
    </div>
</div>